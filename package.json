{
  "name": "node",
  "version": "0.0.0-calibration.1",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "next -p 3004",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest --detectOpenHandles --forceExit --passWithNoTests",
    "test:watch": "jest --watch",
    "export": "next build && next export",
    "serve": "npx serve ./out",
    "store-ipfs": "npx @glif/deployment-cli ipfs ./out $WEB3_STORAGE_TOKEN",
    "publish:calibration": "wrangler publish --env calibration",
    "publish:mainnet": "wrangler publish --env mainnet"
  },
  "dependencies": {
    "@glif/base-css": "^0.0.11",
    "@glif/react-components": "^2.0.0-alpha.62",
    "next": "^12.1.0",
    "prop-types": "15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^18.0.0",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@cloudflare/wrangler": "^1.19.11",
    "@glif/deployment-cli": "^2.0.0-beta.2",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^27.1.1",
    "babel-plugin-styled-components": "2.0.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "^27.1.1",
    "jest-styled-components": "7.0.5",
    "prettier": "2.4.1",
    "react-test-renderer": "17.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "license": "(Apache-2.0 OR MIT)"
}
